{
    "name": "Oof!",
    "version": "1.0",
    "description": "Plays the Roblox oof sound on tab close!",
    "permissions": ["tabs"],
    "background": {
      "scripts": ["main.js"],
      "persistent": false
    },
	"icons": {"48": "icon48.png"},
	"browser_action": {
		"default_icon": "icon48.png"
	},
    "manifest_version": 2
  }
